.body
  width: 80%
  margin: auto
  text-align: center
  display: inline-grid
  grid-template-columns: 1fr 1fr
  grid-template-areas: projects papers

.projects
  width: 100%

.papers
  width: 100%
a:link
  text-decoration: none
  color: black

a:visited
  color: black

.tile
  border: 1px solid black
  text-align: left
  font-size: 1.2em
  width: 40%
  padding: 10px
  margin: 40px auto
  font-family: "Courier New"




